<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Codigo Equipo"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView11"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Detalle"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtDetalleTicket"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Falla"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView8"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView8" />

    <Spinner
        android:id="@+id/spFallaTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/spCodigoEquipoTicket"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/spCodigoEquipoTicket"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/spCodigoEquipoTicket" />

    <EditText
        android:id="@+id/txtDetalleTicket"
        android:layout_width="0dp"
        android:layout_height="80dp"
        android:ems="10"
        android:inputType="textMultiLine"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="5dp"
        app:layout_constraintRight_toRightOf="@+id/btnSigTicket"
        android:layout_marginTop="87dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="93dp"
        app:layout_constraintTop_toTopOf="@+id/tvEstadoTicket"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp" />

    <Button
        android:id="@+id/btnSigTicket"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:onClick="siguienteTicket"
        android:text="Siguiente"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="11dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="11dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Piso"
        app:layout_constraintRight_toLeftOf="@+id/spPisoTicket"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/spPisoTicket"
        android:layout_marginEnd="12dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/spPisoTicket"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="12dp" />

    <Spinner
        android:id="@+id/spPisoTicket"
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Sala"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView7"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView7" />

    <Spinner
        android:id="@+id/spSalaTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/spPisoTicket"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/spPisoTicket"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/spPisoTicket" />

    <Spinner
        android:id="@+id/spCodigoEquipoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/spTipoEquipoTicket"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/spTipoEquipoTicket"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/spTipoEquipoTicket" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:text="Tipo Equipo"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView10"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView10" />

    <Spinner
        android:id="@+id/spTipoEquipoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/spSalaTicket"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/spSalaTicket"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/spSalaTicket" />

    <Spinner
        android:id="@+id/spEstadoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="13dp"
        android:visibility="gone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/spFallaTicket"
        app:layout_constraintRight_toRightOf="@+id/spFallaTicket"
        app:layout_constraintTop_toBottomOf="@+id/spFallaTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tvEstadoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="13dp"
        android:text="Estado"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        app:layout_constraintRight_toRightOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btnEliminar"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:enabled="false"
        android:onClick="eliminar"
        android:text="Eliminar"
        android:visibility="invisible"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/tvRutDocente"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginEnd="9dp"
        android:layout_marginRight="9dp"
        android:text="Rut Docente"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvRutDocenteTicket"
        app:layout_constraintLeft_toLeftOf="@+id/tvEstadoTicket"
        app:layout_constraintRight_toLeftOf="@+id/tvRutDocenteTicket"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvRutDocenteTicket"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="64dp"
        android:layout_marginLeft="64dp"
        android:layout_marginRight="64dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="90dp"
        android:text="12345678-9"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/txtDetalleTicket"
        app:layout_constraintLeft_toLeftOf="@+id/tvEstadoTicket"
        app:layout_constraintRight_toRightOf="@+id/tvRutTecnico"
        app:layout_constraintTop_toTopOf="@+id/textView3"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tvRutTecnico"
        android:layout_width="55dp"
        android:layout_height="35dp"
        android:layout_marginLeft="61dp"
        android:layout_marginStart="61dp"
        android:text="Rut Técnico"
        android:visibility="gone"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvRutDocenteTicket"
        app:layout_constraintLeft_toLeftOf="@+id/spEstadoTicket"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Spinner
        android:id="@+id/spRutTecnicoTicket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="9dp"
        android:layout_marginStart="9dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@+id/tvRutTecnico"
        app:layout_constraintLeft_toRightOf="@+id/tvRutTecnico"
        app:layout_constraintRight_toRightOf="@+id/spEstadoTicket"
        app:layout_constraintTop_toTopOf="@+id/tvRutTecnico"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />
</android.support.constraint.ConstraintLayout>