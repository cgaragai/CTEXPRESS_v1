<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/textView8"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="13dp"
        android:text="Codigo Equipo"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        app:layout_constraintRight_toRightOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:text="Detalle"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtDetalleTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        android:text="Falla"
        app:layout_constraintLeft_toLeftOf="@+id/tvEstadoTicket"
        app:layout_constraintRight_toRightOf="@+id/tvEstadoTicket"
        app:layout_constraintTop_toBottomOf="@+id/textView8"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spFallaTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="12dp"
        app:layout_constraintLeft_toLeftOf="@+id/spCodigoEquipoTicket"
        app:layout_constraintRight_toRightOf="@+id/spCodigoEquipoTicket"
        app:layout_constraintTop_toBottomOf="@+id/spCodigoEquipoTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/txtDetalleTicket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="4dp"
        android:layout_marginTop="353dp"
        android:allowUndo="false"
        android:ems="10"
        android:inputType="textMultiLine"
        android:maxLength="50"
        android:maxLines="3"
        app:layout_constraintBottom_toTopOf="@+id/imageButton"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        app:layout_constraintRight_toRightOf="@+id/spRutTecnicoTicket"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btnSigTicket"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="11dp"
        android:enabled="false"
        android:onClick="siguienteTicket"
        android:text="Siguiente"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="90dp"
        android:layout_height="35dp"
        android:layout_marginBottom="13dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="16dp"
        android:text="Piso"
        app:layout_constraintBottom_toTopOf="@+id/textView10"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spPisoTicket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="64dp"
        android:text="Sala"
        app:layout_constraintLeft_toLeftOf="@+id/textView11"
        app:layout_constraintRight_toRightOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spSalaTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="13dp"
        app:layout_constraintLeft_toLeftOf="@+id/spPisoTicket"
        app:layout_constraintRight_toRightOf="@+id/spPisoTicket"
        app:layout_constraintTop_toBottomOf="@+id/spPisoTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spCodigoEquipoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="13dp"
        app:layout_constraintLeft_toLeftOf="@+id/spTipoEquipoTicket"
        app:layout_constraintRight_toRightOf="@+id/spTipoEquipoTicket"
        app:layout_constraintTop_toBottomOf="@+id/spTipoEquipoTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="9dp"
        android:text="Tipo Equipo"
        app:layout_constraintLeft_toLeftOf="@+id/textView8"
        app:layout_constraintRight_toRightOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spTipoEquipoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="9dp"
        app:layout_constraintLeft_toLeftOf="@+id/spSalaTicket"
        app:layout_constraintRight_toRightOf="@+id/spSalaTicket"
        app:layout_constraintTop_toBottomOf="@+id/spSalaTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spEstadoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="11dp"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/spFallaTicket"
        app:layout_constraintRight_toRightOf="@+id/spFallaTicket"
        app:layout_constraintTop_toBottomOf="@+id/spFallaTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/tvEstadoTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginTop="11dp"
        android:text="Estado"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/tvRutDocente"
        app:layout_constraintRight_toLeftOf="@+id/tvRutDocenteTicket"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/btnEliminar"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginLeft="16dp"
        android:enabled="false"
        android:onClick="eliminar"
        android:text="Eliminar"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/tvRutDocente"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:text="Rut Docente"
        android:visibility="visible"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvRutDocenteTicket"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@+id/textView7"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvRutDocenteTicket"
        android:layout_width="0dp"
        android:layout_height="35dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="12dp"
        android:text="12345678-9"
        android:visibility="visible"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvRutTecnico"
        app:layout_constraintLeft_toRightOf="@+id/tvRutDocente"
        app:layout_constraintRight_toLeftOf="@+id/tvRutTecnico"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/tvRutTecnico"
        android:layout_width="55dp"
        android:layout_height="35dp"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="11dp"
        android:text="Rut Técnico"
        android:visibility="visible"
        app:layout_constraintLeft_toLeftOf="@+id/spEstadoTicket"
        app:layout_constraintTop_toBottomOf="@+id/spEstadoTicket"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Spinner
        android:id="@+id/spRutTecnicoTicket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="10dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/tvRutTecnico"
        app:layout_constraintLeft_toRightOf="@+id/tvRutTecnico"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvRutTecnico"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="119dp"
        android:layout_marginStart="4dp"
        android:onClick="camara"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalleTicket"
        app:srcCompat="@android:drawable/ic_menu_camera"
        android:layout_marginLeft="4dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="27dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:text="(En caso que desee adjuntar foto de la falla)"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toBottomOf="@+id/txtDetalleTicket"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="27dp" />
</android.support.constraint.ConstraintLayout>